on init
    start vendor.weaver_hal_service

service vendor.weaver_hal_service /vendor/bin/hw/android.hardware.weaver-service.thales
    class hal
    user root
    group root drmrpc
    setenv LD_LIBRARY_PATH /vendor/lib64:/vendor/lib64/hw:/system/lib64:/sbin:/odm/lib64
    seclabel u:r:recovery:s0
