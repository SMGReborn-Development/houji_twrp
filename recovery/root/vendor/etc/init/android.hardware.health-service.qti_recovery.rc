service vendor.health-recovery /vendor/bin/hw/android.hardware.health-service.qti_recovery
    class hal
    seclabel u:r:hal_health_default:s0
    user system
    group system
    setenv LD_LIBRARY_PATH /vendor/lib64:/vendor/lib64/hw:/system/lib64:/sbin
    capabilities WAKE_ALARM BLOCK_SUSPEND
    file /dev/kmsg w
